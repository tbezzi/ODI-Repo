def processJsonCMPLX (time, rdd):
  print('========= {0} : {1}, {2} ========='.format (rdd.name(), type(rdd).__name__, str(time)))
  if not rdd.isEmpty():
    try:
      sqlctx = getSqlContextInstance(rdd.context)
